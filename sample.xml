<?xml version="1.0"?>
<survey>
  <title>Sample Quiz</title>
  <author>Matthew Keville</author>

  <question id="q0">
    <prompt>On a scale of 1-10 how much energy do you have right now?</prompt>
    <questionType>SC</questionType>
      <scaleChoice>
        <min>0</min>
        <max>10</max>
        <step>1</step>
        <start>5</start>
      </scaleChoice>
  </question>

  <question id="q1">
    <prompt>Which of the following did you do yesterday?</prompt>
    <questionType>SA</questionType>
    <multipleChoice>
      <option>Clean your room?</option>
      <option>Workout?</option>
      <option>Cook food?</option>
      <option>Play Guitar?</option>
    </multipleChoice>
  </question>


  <question id="q2">
    <prompt>Who was the last person you hung out with?</prompt>
    <questionType>FR</questionType>
    <freeResponse maxLength="200"/>
  </question>

  <question id="q3">
    <prompt>What sounds the most appealing right now?</prompt>
    <questionType>MC</questionType>
    <multipleChoice>
      <option>Going out to drink</option>
      <option>Taking a fat nap</option>
      <option>Playing a video game</option>
      <option>Cuddling up with a good book</option>
    </multipleChoice>
  </question>

  <question id="q4">
    <prompt>How many hours of sleep did you get last night?</prompt>
    <questionType>SC</questionType>
      <scaleChoice>
        <min>0</min>
        <max>12</max>
        <step>1</step>
        <start>6</start>
      </scaleChoice>
  </question>

  <question id="q5">
    <prompt>What are you going to do later tonight?</prompt>
    <questionType>FR</questionType>
    <freeResponse maxLength="200"/>
  </question>


  <question id="q6">
    <prompt>How many cups of coffee did you have today?</prompt>
    <questionType>SC</questionType>
      <scaleChoice>
        <min>0</min>
        <max>10</max>
        <step>1</step>
        <start>0</start>
      </scaleChoice>
  </question>


  <question id="q7">
    <prompt>On a scale of 1-10 how happy are you right now?</prompt>
    <questionType>SC</questionType>
      <scaleChoice>
        <min>0</min>
        <max>10</max>
        <step>1</step>
        <start>0</start>
      </scaleChoice>
  </question>

  <question id="q8">
    <prompt>On a scale of 1-10 how sore are you?</prompt>
    <questionType>SC</questionType>
      <scaleChoice>
        <min>0</min>
        <max>10</max>
        <step>1</step>
        <start>0</start>
      </scaleChoice>
  </question>

  <question id="q9">
    <prompt>Did you workout yesterday?</prompt>
    <questionType>MC</questionType>
    <multipleChoice>
      <option>Yes</option>
      <option>No</option>
    </multipleChoice>
  </question>

  <question id="q10">
    <prompt>Did you cook yesterday?</prompt>
    <questionType>MC</questionType>
    <multipleChoice>
      <option>Yes</option>
      <option>No</option>
    </multipleChoice>
  </question>

  <question id="q10">
    <prompt>Did you meditate yesterday?</prompt>
    <questionType>MC</questionType>
    <multipleChoice>
      <option>Yes</option>
      <option>No</option>
    </multipleChoice>
  </question>

  <question id="q11">
    <prompt>What are you grateful for right now?</prompt>
    <questionType>FR</questionType>
    <freeResponse maxLength="100"/>
  </question>

  <question id="q12">
    <prompt>What made you happy today?</prompt>
    <questionType>FR</questionType>
    <freeResponse maxLength="100"/>
  </question>


</survey>
